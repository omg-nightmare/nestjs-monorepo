{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/nestjs-monorepo/src",
  "monorepo": true,
  "root": "apps/nestjs-monorepo",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/nestjs-monorepo/tsconfig.app.json"
  },
  "projects": {
    "gateway": {
      "type": "application",
      "root": "apps/gateway",
      "entryFile": "main",
      "sourceRoot": "apps/gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gateway/tsconfig.app.json"
      }
    },
    "jwt": {
      "type": "library",
      "root": "libs/jwt",
      "entryFile": "index",
      "sourceRoot": "libs/jwt/src",
      "compilerOptions": {
        "tsConfigPath": "libs/jwt/tsconfig.lib.json"
      }
    },
    "bcrypt": {
      "type": "library",
      "root": "libs/bcrypt",
      "entryFile": "index",
      "sourceRoot": "libs/bcrypt/src",
      "compilerOptions": {
        "tsConfigPath": "libs/bcrypt/tsconfig.lib.json"
      }
    },
    "logger": {
      "type": "library",
      "root": "libs/logger",
      "entryFile": "index",
      "sourceRoot": "libs/logger/src",
      "compilerOptions": {
        "tsConfigPath": "libs/logger/tsconfig.lib.json"
      }
    },
    "member": {
      "type": "application",
      "root": "apps/member",
      "entryFile": "main",
      "sourceRoot": "apps/member/src",
      "compilerOptions": {
        "tsConfigPath": "apps/member/tsconfig.app.json"
      }
    }
  }
}